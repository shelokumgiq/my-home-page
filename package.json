{
  "name": "my-home-page",
  "version": "0.0.1",
  "description": "My home page",
  "private": true,
  "scripts": {
    "lib-init": "lib init",
    "lib-create": "lib create tracking",
    "lib-http": "cd shelokumgiq/tracking && lib http",
    "lib-dev": "cd shelokumgiq/tracking && lib up dev",
    "lib-release": "cd shelokumgiq/tracking && lib release",
    "prebuild-x": "rm -rf docs && cp -R ./assets ./docs",
    "build": "parcel build src/index.html --out-dir docs --no-source-maps --log-level 4",
    "serve": "MS_URI=https://kessiekwang.lib.id MS_TRACK_URI=tracking@dev/track parcel src/index.html --out-dir docs --port $PORT --log-level 4 --hmr-port 8081",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git subtree push --prefix docs deploy master"
  },
  "author": "Shelo Kumgiq",
  "license": "MIT",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "lib.cli": "^4.4.2",
    "parcel-bundler": "^1.10.3",
    "posthtml-expressions": "^1.1.0",
    "posthtml-include": "^1.2.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
